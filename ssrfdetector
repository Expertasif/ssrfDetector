http {
  index index.html;

  server {
    server_name ssrfdetector.com;
    access_log logs/ssrfdetector.access.log main;

    location / {
        proxy_pass http://127.0.0.1:3000;
    }
  }

  server {
    server_name blinkie.xyz;
    access_log  logs/blinkie.access.log main;

    location / {
        proxy_pass http://127.0.0.1:3001;
    }
  }
}
